def abs(n):
    if n < 0:
        return -n
    end
    return n
end

PI = 3.14159

class Point:
    x: Int
    y: Int
    
    Point(x: Int, y: Int):
        this.x = x
        this.y = y
    end
    
    def distance(other: Point) -> Float:
        dx = this.x - other.x
        dy = this.y - other.y
        return Math.sqrt(dx * dx + dy * dy)
    end
end